	<item 
	    android:enabled="true"
	    android:state_pressed="false">   
      <shape>    
         <stroke      
             android:width="2dip"  android:color="#dcdcdc" />  
         <solid android:color="#000000"/>      
         <padding      
             android:left="10dip"  android:top="10dip"      
             android:right="10dip" android:bottom="10dip" />      
      </shape>    
   </item>  
   
	<item 
	    android:enabled="true"
	    android:state_pressed="true">     
      <shape>    
         <stroke      
             android:width="2dip"  android:color="#dcdcdc" />  
         <solid android:color="#fff"/>      
         <padding      
             android:left="5dip"  android:top="5dip"      
             android:right="5dip" android:bottom="5dip" />      
      </shape>    
   </item>  

</selector>